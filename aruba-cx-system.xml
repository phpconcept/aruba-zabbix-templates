                    <name>Ping</name>
                    <type>SIMPLE</type>
                    <key>icmpping</key>
                    <delay>30s</delay>
                    <triggers>
                        <trigger>
                            <uuid>8738508469fb43ff8fb7a55c4477badd</uuid>
